{
  "name": "helix-index-pipelines",
  "version": "3.0.12",
  "private": true,
  "description": "Content extraction pipelines for indexing Project Helix websites\n",
  "scripts": {
    "test": " nyc --reporter=text --reporter=lcov -i -g 'Post-Deploy' --check-coverage --branches 20 --statements 50 --lines 55 mocha",
    "test-ci": "nyc --reporter=text --reporter=lcov -i -g 'Post-Deploy' --check-coverage --branches 20 --statements 50 --lines 55 mocha --reporter xunit --reporter-options output=./junit/test-results.xml && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g 'Post-Deploy'",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "deploy": "node ./build/deploy.js --build --deploy",
    "deploy-sequences": "node ./build/deploy.js -l major -l minor -l latest",
    "deploy-ci": "node ./build/deploy.js --build --deploy --pkgVersion ci$CIRCLE_BUILD_NUM -l ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-index-pipelines.git"
  },
  "wsk": {
    "_0": "NOTE: This project is NOT using the action builder, but uses the same config format.",
    "_1": "      The actions are deployed using the build/deploy.js script.",
    "package": {
      "name": "index-pipelines-v${version}"
    },
    "linksPackage": "index-pipelines"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-index-pipelines/issues"
  },
  "homepage": "https://github.com/adobe/helix-index-pipelines#readme",
  "dependencies": {
    "@adobe/helix-cli": "10.2.4",
    "@adobe/helix-pipeline": "10.0.0",
    "@adobe/helix-shared": "7.6.0",
    "fs-extra": "9.0.1",
    "jsdom": "16.2.2",
    "jsep": "0.3.4",
    "mdast-util-to-string": "1.1.0",
    "moment": "2.26.0",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "unist-util-select": "3.0.1",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "chalk": "4.0.0",
    "codecov": "3.7.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.2.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.21.1",
    "glob": "7.1.6",
    "junit-report-builder": "2.0.0",
    "mocha": "8.0.1",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "openwhisk": "3.21.2",
    "semantic-release": "17.0.8",
    "semver": "7.3.2",
    "yargs": "15.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
